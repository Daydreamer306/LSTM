		},
		{
			"label": "Data Preprocessing",
			"type": "shell",
			"command": "python",
			"args": [
				"src/data/preprocess.py"
			],
			"group": "build",
			"isBackground": false,
			"problemMatcher": []
		},
		{
			"label": "Model Inference",
			"type": "shell",
			"command": "python",
			"args": [
				"src/inference/predict.py",
				"--model_path",
				"/mnt/workspace/models/best_model.pt"
			],
			"group": "test",
			"isBackground": false,
			"problemMatcher": []
		},
		{
